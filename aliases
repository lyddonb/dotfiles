alias aa='vim ~/Dropbox/dotfiles/aliases && . ~/Dropbox/dotfiles/aliases'

alias home='cd ~'

#GIT
#alias rebase='git svn rebase'
#alias dcommit='git svn dcommit'
alias gs='git status'
alias gpull='git pull'
alias gpush='git push'
alias gb='git branch'

lookfor() {
    grep -R --include="*.py" $@ *
}
alias pgrep='lookfor'
alias gpgrep="git grep | grep '\.py:'"
lookforjs() {
    grep -R --include="*.js" $@ *
}
alias jgrep='lookforjs'


#python
alias p='python'


#personal gae
alias ntest='nosetests -d --with-gae --without-sandbox -v -l DEBUG --nocapture'

#spicolis
alias spicolis='cd ~/Dropbox/Spicolis/spicolis'
